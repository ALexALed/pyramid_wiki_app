#!/home/alexaled/dev/pyramid_wiki_app/.env/bin/python3.5
# EASY-INSTALL-ENTRY-SCRIPT: 'pyramid-wiki-app','console_scripts','initialize_pyramid_wiki_app_db'
__requires__ = 'pyramid-wiki-app'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pyramid-wiki-app', 'console_scripts', 'initialize_pyramid_wiki_app_db')()
    )
